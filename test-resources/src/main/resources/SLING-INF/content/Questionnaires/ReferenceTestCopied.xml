        </property>
        <property>
            <name>dataType</name>
            <value>text</value>
            <type>String</type>
        </property>
        <property>
            <name>compact</name>
            <value>True</value>
            <type>Boolean</type>
        </property>
        <property>
            <name>entryMode</name>
            <value>reference</value>
            <type>String</type>
        </property>
        <property>
            <name>displayMode</name>
            <value>plain</value>
            <type>String</type>
        </property>
        <property>
            <name>question</name>
            <value>/Questionnaires/ReferenceTestUser/textQuestion</value>
            <type>String</type>
        </property>
        <property>
            <name>maxAnswers</name>
            <value>1</value>
            <type>Long</type>
        </property>
    </node>
    <node>
        <name>testComputed</name>
        <primaryNodeType>cards:Question</primaryNodeType>
