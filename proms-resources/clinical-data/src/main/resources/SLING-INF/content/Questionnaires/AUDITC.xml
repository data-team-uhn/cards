<node>
	<name>AUDITC</name>
	<primaryNodeType>cards:Questionnaire</primaryNodeType>
	<property>
		<name>title</name>
		<value>Alcohol Use Disorders Identification Test</value>
		<type>String</type>
	</property>
	<property>
		<name>requireCompletion</name>
		<value>True</value>
		<type>Boolean</type>
