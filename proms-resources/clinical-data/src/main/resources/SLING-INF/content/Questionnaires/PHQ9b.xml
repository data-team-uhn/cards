		<name>phq9_score</name>
		<primaryNodeType>cards:Question</primaryNodeType>
		<property>
			<name>maxAnswers</name>
			<value>1</value>
			<type>Long</type>
		</property>
		<property>
			<name>dataType</name>
			<value>text</value>
			<type>String</type>
		</property>
		<property>
			<name>text</name>
			<value>PHQ-9 Score</value>
			<type>String</type>
		</property>
		<property>
			<name>expression</name>
			<value>return @{phq9_2:-0} + @{phq9_3:-0} + @{phq9_4:-0} + @{phq9_5:-0} + @{phq9_6:-0} + @{phq9_7:-0} + @{phq9_8:-0} + @{phq9_9:-0} + @{phq9_10:-0}</value>
			<type>String</type>
		</property>
		<property>
			<name>unitOfMeasurement</name>
			<value>points</value>
			<type>String</type>
		</property>
		<property>
			<name>entryMode</name>
			<value>computed</value>
			<type>String</type>
		</property>
	</node>
	<node>
		<name>phq9_classification</name>
		<primaryNodeType>cards:Question</primaryNodeType>
		<property>
			<name>maxAnswers</name>
			<value>1</value>
			<type>Long</type>
		</property>
		<property>
			<name>dataType</name>
			<value>text</value>
			<type>String</type>
		</property>
		<property>
			<name>text</name>
			<value>PHQ-9 Calssification</value>
			<type>String</type>
		</property>
		<property>
			<name>expression</name>
			<value><![CDATA[return (
   4 >= @{phq9_score} ? "Minimal"
:  9 >= @{phq9_score} ? "Mild"
: 14 >= @{phq9_score} ? "Moderate"
: 19 >= @{phq9_score} ? "Moderately severe"
:  "Severe"
)]]></value>
			<type>String</type>
		</property>
		<property>
			<name>entryMode</name>
			<value>computed</value>
			<type>String</type>
		</property>
	</node>
